{
	"name": "chatgpt-cli",
	"version": "0.0.1",
	"description": "Run ChatGPT in the terminal",
	"main": "./dist/index.js",
	"scripts": {
		"start": "tsc --watch",
		"build": "tsc",
		"test": "echo \"Error: no test specified\" && exit 1",
